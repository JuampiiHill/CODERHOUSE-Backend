<div>
	<h1>Lista de Productos</h1>
	{{#if allProducts}}
		{{#each allProducts}}
			<h2>{{this.title}}</h2>
			<p>Descripci√≥n: <strong>{{this.description}}</strong></p>
			<p>Codigo del producto: <strong>{{this.code}}</strong></p>
			<p>Precio: <strong>{{this.price}}</strong></p>
			<p>Stock: <strong>{{this.stock}}</strong></p>
			<p>Categoria: <strong>{{this.category}}</strong></p>
			{{#if this.thumbnails}}
			{{/if}}
		{{/each}}
	{{else}}
		<h3>No hay productos agregados recientemente</h3>
	{{/if}}
</div>